<!--pages/resulting/resulting.wxml-->
<view class="page">
  <view class="page__bd">
    <view class="weui-panel weui-panel_access">
      <view class="weui-panel__bd">
        <view class="weui-media-box weui-media-box_text">
          <view class="weui-media-box__title weui-media-box__title_in-text">{{info.member_id_name}} {{info.mdate}} {{info.mtype_name}}</view>
          <view class="weui-media-box__title weui-media-box__title_in-text"> {{info.contact}}</view>
          <view class="weui-media-box__desc">
            <text>{{info.description}}</text>
          </view>
          <view class="weui-media-box__desc" wx:if="{{info.contact.length>0}}">
            <text>{{info.contact}}</text>
          </view>
        </view>
      </view>

    </view>

    <view class="weui-msg__extra-area" >
      <button class="weui-btn" bindtap='startMeeting' type="primary"  wx:if="{{info.status=='W'}}" style='margin-left:15px;margin-right:15px;'>开始会诊</button>
    </view>

  </view>

</view>